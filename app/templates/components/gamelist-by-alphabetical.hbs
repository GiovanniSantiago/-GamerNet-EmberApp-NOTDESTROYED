<!--POPULAR DROPDOWN-->
<div class="panel panel-default">
	<div class="panel-heading">
		<p class="panel-title">
			GAMES :O
		</p>
	</div>
	<ul class="list-group">
		{{#each gamelist as |game|}}
			<li class="list-group-item">{{#link-to "game" game.game_id}}ID: {{game.name}}{{/link-to}}</li>
		{{else}}
			<li class="list-group-item text-muted">No games ;^;</li>
		{{/each}}
	</ul>
</div>
