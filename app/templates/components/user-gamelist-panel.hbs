			<li class="list-group-item">
				{{#link-to 'game' game}}{{game}}{{/link-to}}
				<!-- 
				{{#link-to 'game' game.game_id}}{{game.name}}{{/link-to}}
				-->
			</li>
		{{else}}
			<li class="list-group-item text-muted">
				I'm grupy-pants and like no games ;^;
			</li>
		{{/each}}
